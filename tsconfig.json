{
  "extends": "@react-native/typescript-config",
    "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@components/*": ["components/*"],
      "@screens/*": ["screens/*"],
      "@utils/*": ["utils/*"],
      "@hooks": ["hooks/index"],
      "@hooks/*": ["hooks/*"],
      "@constants/*": ["constants/*"],
      "@navigation/*": ["navigation/*"],
      "@services/*": ["services/*"],
      "@types": ["types/index"],
      "@types/*": ["types/*"]

    }
  }
}
